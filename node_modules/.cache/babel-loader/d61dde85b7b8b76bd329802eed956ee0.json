oOA;AACA;AACA,SAAA,eAAA;AACA,SAAA,WAAA;AACA;AAxOA,CAAA","sourcesContent":["<template>\r\n\t<!-- 新增医生弹窗 -->\r\n\t<el-dialog width=\"70%\" :title=\"$t('default.MODULE.DEPARTMENT.ADD_DOCTOR')\" :visible.sync=\"layzValue\" top=\"10vh\">\r\n\t\t<!-- 内层新增医院弹窗 -->\r\n\t\t<el-dialog\r\n\t\t\twidth=\"30%\"\r\n\t\t\t:title=\"$t('default.MODULE.DEPARTMENT.ADD_HOSPITAL')\"\r\n\t\t\t:visible.sync=\"view.hospital\"\r\n\t\t\tappend-to-body>\r\n\t\t\t<el-form :model=\"hospitalInfo\" :rules=\"rules\" ref=\"hospitalForm\">\r\n\t\t\t\t<!-- 医院名称 -->\r\n\t\t\t\t<el-form-item :label=\"$t('default.MODULE.DEPARTMENT.HOSPITAL_NAME')\" prop=\"HospitalName\" label-width=\"90px\">\r\n\t\t\t\t\t<el-input v-model=\"hospitalInfo.HospitalName\" :placeholder=\"$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:$t('default.MODULE.DEPARTMENT.HOSPITAL_NAME')})\" maxlength=\"10\" show-word-limit autocomplete=\"off\" style=\"width: 300px;\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<!-- 医院说明 -->\r\n\t\t\t\t<el-form-item :label=\"$t('default.BASIC.DESCRIPTION')\" prop=\"HospitalDescription\" label-width=\"90px\">\r\n\t\t\t\t\t<el-input v-model=\"hospitalInfo.HospitalDescription\" resize=\"none\" type=\"textarea\" :placeholder=\"$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:$t('default.BASIC.DESCRIPTION')})\" maxlength=\"150\" show-word-limit style=\"width: 300px;\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</el-form>\r\n\t\t\t<div slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t\t<el-button @click=\"view.hospital = false\">{{ $t('default.BASIC.CANCEL') }}</el-button>\r\n\t\t\t\t<el-button type=\"primary\" @click=\"hospitalEnter()\">{{ $t('default.BASIC.ENTER') }}</el-button>\r\n\t\t\t</div>\r\n\t\t</el-dialog>\r\n\t\t<!-- 新增医生内容 -->\r\n\t\t<el-row type=\"flex\">\r\n\t\t\t<el-col :span=\"4\"></el-col>\r\n\t\t\t<el-col :span=\"18\">\r\n\t\t\t\t<!-- <el-upload\r\n\t\t\t\t\tstyle=\"text-align: center;margin-bottom: 20px;\"\r\n\t\t\t\t\tclass=\"avatar-uploader\"\r\n\t\t\t\t\t:data=\"headImgData\"\r\n\t\t\t\t\t:action=\"uploadUrl\"\r\n\t\t\t\t\t:show-file-list=\"false\"\r\n\t\t\t\t\t:on-success=\"handleAvatarSuccess\"\r\n\t\t\t\t\t:before-upload=\"beforeAvatarUpload\">\r\n\t\t\t\t\t<img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\">\r\n\t\t\t\t\t<i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n\t\t\t\t</el-upload> -->\r\n\t\t\t\t<el-form :model=\"doctorInfo\" :rules=\"rules\" ref=\"doctorForm\">\r\n\t\t\t\t\t<!-- 登录账号 -->\r\n\t\t\t\t\t<el-form-item v-if=\"!isEdit\" :label=\"$t('default.BASIC.ACCOUNT')\" prop=\"DoctorLoginCode\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-input v-model=\"doctorInfo.DoctorLoginCode\" :placeholder=\"$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:$t('default.BASIC.ACCOUNT')})\" maxlength=\"20\" show-word-limit autocomplete=\"off\" style=\"width: 300px;\"></el-input>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<el-form-item v-if=\"isEdit\" :label=\"$t('default.BASIC.ACCOUNT')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<span>{{ doctorInfo.UserCode }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 医生姓名 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.MODULE.DEPARTMENT.DOCTOR_NAME')\" prop=\"DoctorName\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-input v-model=\"doctorInfo.DoctorName\" :placeholder=\"$t('default.MODULE.DEPARTMENT.INPUT_DOCTOR_NAME')\" maxlength=\"10\" show-word-limit autocomplete=\"off\" style=\"width: 300px;\"></el-input>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 性别 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.INFOS.GENDE')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-radio-group v-model=\"doctorInfo.Gender\">\r\n\t\t\t\t\t\t\t<el-radio :label=\"$t('default.INFOS.MAN')\">{{ $t('default.INFOS.MAN') }}</el-radio>\r\n\t\t\t\t\t\t\t<el-radio :label=\"$t('default.INFOS.WOMAN')\">{{ $t('default.INFOS.WOMAN') }}</el-radio>\r\n\t\t\t\t\t\t</el-radio-group>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 所属科室 -->\r\n\t\t\t\t\t<el-form-item v-if=\"!isEdit\" :label=\"$t('default.MODULE.DEPARTMENT.AFFILIATED_DEPARTMENT')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-select v-model=\"doctorInfo.OfficeTypeGuid\" multiple collapse-tags :placeholder=\"$t('default.TIPS.CLIENT.SELECT_SOMETHING',{name:$t('default.MODULE.DEPARTMENT.AFFILIATED_DEPARTMENT')})\" style=\"width: 300px;\">\r\n\t\t\t\t\t\t\t<!-- <el-option :label=\"$t('default.EVENTS.UNSELECT')\" :value=\"$null\"></el-option> -->\r\n\t\t\t\t\t\t\t<!-- <el-option v-for=\"(item,index) in departmentNode\" :key=\"index\" :label=\"item.OfficeTypeName\" :value=\"item.OfficeTypeGuid\"></el-option> -->\r\n\t\t\t\t\t\t\t<el-option-group\r\n\t\t\t\t\t\t\t\tv-for=\"(item,index) in department\"\r\n\t\t\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t\t\t:label=\"item.OfficeTypeName\">\r\n\t\t\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\t\t\tv-for=\"(sub,subIndex) in item.SubTypeList\"\r\n\t\t\t\t\t\t\t\t\t:key=\"subIndex\"\r\n\t\t\t\t\t\t\t\t\t:label=\"sub.OfficeTypeName\"\r\n\t\t\t\t\t\t\t\t\t:value=\"sub.OfficeTypeGuid\">\r\n\t\t\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t\t\t</el-option-group>\r\n\t\t\t\t\t\t</el-select>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 所属医院 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.MODULE.DEPARTMENT.AFFILIATED_HOSPITAL')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-select v-model=\"doctorInfo.HospitalGuid\" :placeholder=\"$t('default.TIPS.CLIENT.SELECT_SOMETHING',{name:$t('default.MODULE.DEPARTMENT.AFFILIATED_HOSPITAL')})\" >\r\n\t\t\t\t\t\t\t<el-option :label=\"$t('default.EVENTS.UNSELECT')\" :value=\"$null\"></el-option>\r\n\t\t\t\t\t\t\t<el-option v-for=\"(item,index) in hospitalData\" :key=\"index\" :label=\"item.HospitalName\" :value=\"item.HospitalGuid\"></el-option>\t\r\n\t\t\t\t\t\t</el-select>\r\n\t\t\t\t\t\t<el-button slot=\"append\" icon=\"el-icon-search\"></el-button>\r\n\t\t\t\t\t\t<el-button @click=\"addHospital\" size=\"small\" type=\"success\" icon=\"el-icon-plus\" circle style=\"position: absolute;margin-left: 10px;margin-top: 5px;\"></el-button>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 医生照片 -->\r\n\t\t\t\t\t<el-form-item label=\"医生照片\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-upload\r\n\t\t\t\t\t\t\tclass=\"avatar-uploader\"\r\n\t\t\t\t\t\t\t:action=\"uploadUrl\"\r\n\t\t\t\t\t\t\t:data=\"uploadData\"\r\n\t\t\t\t\t\t\taccept=\"image/jpeg, image/jpg\"\r\n\t\t\t\t\t\t\t:show-file-list=\"false\"\r\n\t\t\t\t\t\t\t:multiple=\"false\"\r\n\t\t\t\t\t\t\t:limit=\"1\"\r\n\t\t\t\t\t\t\t:on-success=\"onImgUpload\">\r\n\t\t\t\t\t\t\t<div v-if=\"doctorInfo.ImagePath\" class=\"avatar-box\">\r\n\t\t\t\t\t\t\t\t<div class=\"avatar-zoom\">\r\n\t\t\t\t\t\t\t\t\t<i class=\"el-icon-delete\" @click.stop=\"doctorInfo.ImagePath=''\"></i>\r\n\t\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t\t<img :src=\"serverAddr+doctorInfo.ImagePath\" class=\"avatar\">\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t<i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n\t\t\t\t\t\t</el-upload>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 说明 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.BASIC.DESCRIPTION')\" prop=\"DoctorIntroduction\" label-width=\"100px\">\r\n\t\t\t\t\t\t<div ref=\"description\" style=\"height:250px\"></div>\r\n\t\t\t\t\t\t<!-- <el-input v-model=\"doctorInfo.DoctorDescription\" resize=\"none\" type=\"textarea\" :placeholder=\"$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:$t('default.BASIC.DESCRIPTION')})\" maxlength=\"150\" show-word-limit style=\"width: 300px;\"></el-input> -->\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-form>\r\n\t\t\t</el-col>\r\n\t\t</el-row>\r\n\t\t<div slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t<el-button @click=\"layzValue = false\">{{ $t('default.BASIC.CANCEL') }}</el-button>\r\n\t\t\t<el-button type=\"primary\" @click=\"doctorEnter()\">{{ $t('default.BASIC.ENTER') }}</el-button>\r\n\t\t</div>\r\n\t</el-dialog>\r\n</template>\r\n\r\n<script>\r\n\timport conf from '@/conf/conf.js'\r\n\timport E from 'wangeditor'\r\n\texport default {\r\n\t\tname:\"docotr\",\r\n\t\tprops:{\r\n\t\t\tisEdit:{\r\n\t\t\t\ttype:Boolean,\r\n\t\t\t\tdefault:false,\r\n\t\t\t},\r\n\t\t\tvalue:{\r\n\t\t\t\ttype:Boolean,\r\n\t\t\t\tdefault:false,\r\n\t\t\t},\r\n\t\t\tdocotrGuid:{\r\n\t\t\t\ttype:String,\r\n\t\t\t\tdefault:\"\"\r\n\t\t\t}\r\n\t\t},\r\n\t\twatch:{\r\n\t\t\tvalue(){\r\n\t\t\t\tthis.layzValue = this.value;\r\n\t\t\t\t\r\n\t\t\t\tif(this.value && this.isFirst){\r\n\t\t\t\t\tthis.isFirst = false;\r\n\t\t\t\t\tsetTimeout(()=>{\r\n\t\t\t\t\t\tthis.initEdit();\r\n\t\t\t\t\t},200)\t\t\t\t\r\n\t\t\t\t}\r\n\t\t\t\tif(this.value && !this.isEdit){\r\n\t\t\t\t\tsetTimeout(()=>{\r\n\t\t\t\t\t\tthis.editor.txt.clear();\r\n\t\t\t\t\t},200)\r\n\t\t\t\t\t\r\n\t\t\t\t\tthis.doctorInfo={\r\n\t\t\t\t\t\tDoctorName:\"\",\r\n\t\t\t\t\t\tDoctorLoginCode:\"\",\r\n\t\t\t\t\t\tGender:this.$t('default.INFOS.MAN'),\r\n\t\t\t\t\t\tHospitalGuid:\"\",\r\n\t\t\t\t\t\tOfficeTypeGuid:[],\r\n\t\t\t\t\t\tDoctorDescription:\"\",\r\n\t\t\t\t\t\tImagePath:\"\",\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tlayzValue(){\r\n\t\t\t\tthis.$emit(\"input\",this.layzValue);\r\n\t\t\t},\r\n\t\t\tdocotrGuid(){\r\n\t\t\t\tif(this.docotrGuid.length > 0 && this.isEdit){\r\n\t\t\t\t\tthis.queryDoctorDetail();\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t},\r\n\t\tdata(){\r\n\t\t\treturn {\r\n\t\t\t\tuploadUrl:conf.getCurrentUploadAddr(),\r\n\t\t\t\tserverAddr:conf.getCurrentServeAddr(\"main\"),\r\n\t\t\t\tuploadData:{\r\n\t\t\t\t\tmode:\"upload\"\r\n\t\t\t\t},\r\n\t\t\t\tview:{\r\n\t\t\t\t\tdoctor:false,\r\n\t\t\t\t\thospital:false,\r\n\t\t\t\t},\r\n\t\t\t\thospitalInfo:{\r\n\t\t\t\t\tHospitalName:\"\",\r\n\t\t\t\t\tHospitalDescription:\"\"\r\n\t\t\t\t},\r\n\t\t\t\tdoctorInfo:{\r\n\t\t\t\t\tDoctorName:\"\",\r\n\t\t\t\t\tDoctorLoginCode:\"\",\r\n\t\t\t\t\tGender:this.$t('default.INFOS.MAN'),\r\n\t\t\t\t\tHospitalGuid:\"\",\r\n\t\t\t\t\tOfficeTypeGuid:[],\r\n\t\t\t\t\tDoctorDescription:\"\",\r\n\t\t\t\t\tDoctorIntroduction:\"\",\r\n\t\t\t\t\tImagePath:\"\",\r\n\t\t\t\t},\r\n\t\t\t\tisFirst:true,\r\n\t\t\t\tdepartment:[],\r\n\t\t\t\thospitalData:[],\r\n\t\t\t\tlayzValue:false,\r\n\t\t\t\teditor:{},\r\n\t\t\t\trules:{\r\n\t\t\t\t\tOfficeTypeName:[\r\n\t\t\t\t\t\t{ required: true, message: this.$t(\"default.MODULE.DEPARTMENT.INPUT_DEPARTMENT_NAME\"), trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tHospitalName:[\r\n\t\t\t\t\t\t{ required: true, message: this.$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:this.$t('default.MODULE.DEPARTMENT.HOSPITAL_NAME')}), trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tDoctorLoginCode:[\r\n\t\t\t\t\t\t{ required: true, message: this.$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:this.$t('default.BASIC.ACCOUNT')}), trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tDoctorName:[\r\n\t\t\t\t\t\t{ required: true, message: this.$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:this.$t('default.MODULE.DEPARTMENT.DOCTOR_NAME')}), trigger: 'blur' },\r\n\t\t\t\t\t]\r\n\t\t\t\t},\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods:{\r\n\t\t\tonImgRemove(e){\r\n\t\t\t\tthis.doctorInfo.ImagePath = \"\";\r\n\t\t\t},\r\n\t\t\tonImgUpload(e){\r\n\t\t\t\tconsole.log(e)\r\n\t\t\t\t//let data = JSON.parse(e.Data)\r\n\t\t\t\tthis.doctorInfo.ImagePath = e.Data.Path;\r\n\t\t\t},\r\n\t\t\tinitEdit(){\r\n\t\t\t\tthis.editor = new E(this.$refs.description)\r\n\t\t\t\tthis.editor.create()\r\n\t\t\t},\r\n\t\t\tdoctorEnter(){\r\n\t\t\t\t//验证医生表单\r\n\t\t\t\tthis.$refs.doctorForm.validate((valid)=>{\r\n\t\t\t\t\tif(valid){\r\n\t\t\t\t\t\tif(this.isEdit){\r\n\t\t\t\t\t\t\tthis.saveDoctorDetail()\r\n\t\t\t\t\t\t}else{\r\n\t\t\t\t\t\t\tthis.createDoctor();\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\treturn false;\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\thospitalEnter(){\r\n\t\t\t\t//验证医院表单\r\n\t\t\t\tthis.$refs.doctorForm.validate((valid)=>{\r\n\t\t\t\t\tif(valid){\r\n\t\t\t\t\t\tthis.createHospital();\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\treturn false;\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\t/*添加医院*/\r\n\t\t\taddHospital(){\r\n\t\t\t\tthis.hospitalInfo.HospitalName = \"\";\r\n\t\t\t\tthis.hospitalInfo.HospitalDescription = \"\";\r\n\t\t\t\tthis.view.hospital = true;\r\n\t\t\t},\r\n\t\t\t/*创建医院*/\r\n\t\t\tasync createHospital(){\r\n\t\t\t\tlet params = {\r\n\t\t\t\t\tHospitalName:this.hospitalInfo.HospitalName,\r\n\t\t\t\t\tHospitalDescription:this.hospitalInfo.HospitalDescription,\r\n\t\t\t\t}\r\n\t\t\t\tlet res = await this.$api.bus.Medical.createHospital(params)\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\tthis.$tips.warning(\"创建失败\");\r\n\t\t\t\t\treturn\r\n\t\t\t\t}\r\n\t\t\t\tthis.$tips.success(\"创建成功\");\r\n\t\t\t\tthis.view.hospital = false;\r\n\t\t\t\tthis.getHospital();\r\n\t\t\t},\r\n\t\t\t/*查询医院*/\r\n\t\t\tasync getHospital(){\r\n\t\t\t\tlet params = {\r\n\t\t\t\t\tPageIndex:1,\r\n\t\t\t\t\tPageSize:20,\r\n\t\t\t\t\tRowFilter:\"\"\r\n\t\t\t\t}\r\n\t\t\t\tlet res = await this.$api.bus.Medical.queryHospital(params)\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\tthis.$tips.warning(\"查询失败\");\r\n\t\t\t\t\treturn\r\n\t\t\t\t}\t\r\n\t\t\t\tthis.hospitalData = res.Data.DataList;\r\n\t\t\t},\r\n\t\t\t/*查询科室*/\r\n\t\t\tasync queryOfficeType(){\r\n\t\t\t\tlet res = await this.$api.bus.Medical.queryOfficeType();\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\treturn\r\n\t\t\t\t}\r\n\t\t\t\tthis.department = res.Data.DataList;\r\n\t\t\t},\r\n\t\t\t//获取医生详情\r\n\t\t\tasync queryDoctorDetail(){\r\n\t\t\t\tlet params = {\r\n\t\t\t\t\tTargetUserGuid:this.docotrGuid\r\n\t\t\t\t}\r\n\t\t\t\tlet res = await this.$api.get(\"/Medical/GetDoctorDetail\",params)\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\tthis.$tips.warning(\"查询数据异常\")\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tthis.doctorInfo = res.Data;\r\n\t\t\t\tsetTimeout(()=>{\r\n\t\t\t\t\tif(this.editor)\r\n\t\t\t\t\t\tthis.editor.txt.html(this.doctorInfo.DoctorIntroduction);\r\n\t\t\t\t},1000)\r\n\t\t\t\t\r\n\t\t\t\t//this.dotocrDetail = res.Data;\r\n\t\t\t},\r\n\t\t\t/*创建医生*/\r\n\t\t\tasync createDoctor(){\r\n\t\t\t\tlet params = Object.assign({},this.doctorInfo);\r\n\t\t\t\tparams.OfficeTypeGuid = params.OfficeTypeGuid.join(\",\");\r\n\t\t\t\tparams.DoctorIntroduction = this.editor.txt.html();\r\n\t\t\t\tparams.Heading = params.ImagePath;\r\n\t\t\t\t//params.OfficeTypeGuid = JSON.stringify(params.OfficeTypeGuid);\r\n\t\t\t\tlet res = await this.$api.bus.Medical.createDoctor(params);\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\tthis.$tips.warning(\"创建失败\");\r\n\t\t\t\t\treturn\r\n\t\t\t\t}\r\n\t\t\t\tthis.$tips.success(\"创建成功\");\r\n\t\t\t\tthis.layzValue = false;\r\n\t\t\t\tthis.$emit(\"doctorCreate\")\r\n\t\t\t\tthis.$emit(\"doctorChange\")\r\n\t\t\t},\r\n\t\t\t//更新医生详情\r\n\t\t\tasync saveDoctorDetail(){\r\n\t\t\t\tlet params = this.doctorInfo;\r\n\t\t\t\tparams.DoctorIntroduction = this.editor.txt.html()\r\n\t\t\t\tparams.HeadImg = params.ImagePath;\r\n\t\t\t\tlet res = await this.$api.post(\"/Medical/SaveDoctor\",params)\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\tthis.$tips.warning(\"更新错误\")\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tthis.$tips.success(\"更新成功\");\r\n\t\t\t\tthis.layzValue = false;\r\n\t\t\t\tthis.$emit(\"doctorUpdate\")\r\n\t\t\t\tthis.$emit(\"doctorChange\")\r\n\t\t\t}\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\t//this.initEdit();\r\n\t\t\tthis.queryOfficeType();\r\n\t\t\tthis.getHospital();\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.w-e-text-container{\r\n\t\theight: 250px !important;/*!important是重点，因为原div是行内样式设置的高度300px*/\r\n\t}\r\n\t.avatar-uploader .el-upload {\r\n\t\tborder: 1px dashed #d9d9d9;\r\n\t\tborder-radius: 6px;\r\n\t\tcursor: pointer;\r\n\t\tposition: relative;\r\n\t\toverflow: hidden;\r\n\t}\r\n\t.avatar-uploader .el-upload:hover {\r\n\t\tborder-color: #409EFF;\r\n\t}\r\n\t.avatar-uploader-icon {\r\n\t\tfont-size: 28px;\r\n\t\tcolor: #8c939d;\r\n\t\twidth: 178px;\r\n\t\theight: 178px;\r\n\t\tline-height: 178px;\r\n\t\ttext-align: center;\r\n\t}\r\n\t.avatar {\r\n\t\twidth: 178px;\r\n\t\theight: 178px;\r\n\t\tdisplay: block;\r\n\t}\r\n\t.avatar-box:hover .avatar-zoom{\r\n\t\tvisibility: visible;\r\n\t}\r\n\t.avatar-zoom{\r\n\t\tvisibility: hidden;\r\n\t\tposition: absolute;background: rgba(0, 0, 0, 0.5);color: rgb(255, 255, 255);width: 100%;height: 100%;line-height: 6;font-size: 28px;\r\n\t}\r\n</style>\r\n"],"sourceRoot":"pages/main/account"}]}