\"selectHealthRecord.MedicalRecordGuid\"></health-record-detail>\r\n\t\t</el-dialog>\r\n\t\t<!-- 切换支付状态 -->\r\n\t\t<el-dialog title=\"支付状态\" :visible.sync=\"view.payStatus\">\r\n\t\t\t确定切换为{{ selectCustomer.PayStatus == 0?'已支付':'待支付' }}\r\n\t\t\t<div slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t\t<el-button @click=\"view.payStatus = false\">{{ $t('default.BASIC.CANCEL') }}</el-button>\r\n\t\t\t\t<el-button type=\"primary\" @click=\"updateAppointmentPayStatus()\">{{ $t('default.BASIC.ENTER') }}</el-button>\r\n\t\t\t</div>\r\n\t\t</el-dialog>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport healthRecordDetail from \"@/pages/main/appointment/health_record_detail.vue\";\r\n\r\n\texport default {\r\n\t\tcomponents: {\r\n\t\t\thealthRecordDetail\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tview:{\r\n\t\t\t\t\ttableHeight:650,\r\n\t\t\t\t\tpayStatus:false,\r\n\t\t\t\t\tdataIsLoading:false,\r\n\t\t\t\t},\r\n\t\t\t\tallData: [],\r\n\t\t\t\tselectHealthRecord: {},\r\n\t\t\t\tseekAppointmentData: {\r\n\t\t\t\t\tdata: [],\r\n\t\t\t\t\ttotal: 0\r\n\t\t\t\t},\r\n\t\t\t\tgoJpaAppointmentData: {\r\n\t\t\t\t\tdata: [],\r\n\t\t\t\t\ttotal: 0\r\n\t\t\t\t},\r\n\t\t\t\tselectCustomer:{},\r\n\t\t\t\tactiveName: \"Examine\",\r\n\t\t\t\tfeeAmount: \"\",\r\n\t\t\t\thealthRecordDtlDialog: false,\r\n\t\t\t\tconfirmDialog: false,\r\n\t\t\t\tcurrentObj: null,\r\n\t\t\t\tconfirmObj: {\r\n\t\t\t\t\tGaeaObjectProperty: 0,\r\n\t\t\t\t\tConfirmResult: null,\r\n\t\t\t\t\tFeeAmount: \"\",\r\n\t\t\t\t\tFailResult: \"\"\r\n\t\t\t\t},\r\n\t\t\t\tkeyword:\"\",\r\n\t\t\t\tpages:{\r\n\t\t\t\t\tpage:1,\r\n\t\t\t\t\tsize:20,\r\n\t\t\t\t\ttotal:0,\r\n\t\t\t\t},\r\n\t\t\t\trules: {\r\n\t\t\t\t\tConfirmResult: [{\r\n\t\t\t\t\t\trequired: true,\r\n\t\t\t\t\t\tmessage: \"请选择预约结果\",\r\n\t\t\t\t\t\ttrigger: \"change\"\r\n\t\t\t\t\t}],\r\n\t\t\t\t\t// FeeAmount: [\r\n\t\t\t\t\t//   { required: true, message: \"请输入费用金额\", trigger: \"blur\" }\r\n\t\t\t\t\t// ],\r\n\t\t\t\t\tFailResult: [{\r\n\t\t\t\t\t\trequired: true,\r\n\t\t\t\t\t\tmessage: \"请填写失败原因\",\r\n\t\t\t\t\t\ttrigger: \"blur\"\r\n\t\t\t\t\t}]\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t},\r\n\t\tcomputed: {\r\n\t\t\tisReadonly() {\r\n\t\t\t\tif (!this.currentObj) return false;\r\n\t\t\t\treturn (this.currentObj.GaeaObjectProperty & 524289) > 0;\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.view.tableHeight = window.innerHeight/100 * 75;\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\tthis.queryData();\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\tpageSizeChange(e){\r\n\t\t\t\tthis.pages.size = e;\r\n\t\t\t\tthis.pages.page = 1;\r\n\t\t\t\tthis.queryData()\r\n\t\t\t},\r\n\t\t\tpagesIndexChange(e){\r\n\t\t\t\tthis.pages.page = e;\r\n\t\t\t\tthis.queryData()\r\n\t\t\t},\r\n\t\t\tsearch(){\r\n\t\t\t\tthis.pages.page = 1;\r\n\t\t\t\tthis.queryData()\r\n\t\t\t},\r\n\t\t\tswitchFeeStatus(data) {\r\n\t\t\t\t\r\n\t\t\t\tthis.selectCustomer = data;\r\n\t\t\t\tthis.view.payStatus = true;\r\n\t\t\t},\r\n\t\t\tformatRecept(row, column, cellValue, index) {\r\n\t\t\t\tif ((row.GaeaObjectProperty & 2) == 2) return \"是\";\r\n\r\n\t\t\t\treturn \"否\";\r\n\t\t\t},\r\n\t\t\tformatStatus(row) {\r\n\t\t\t\tlet statusName = \"\";\r\n\t\t\t\t//TODO 状态值转换待优化\r\n\t\t\t\tif ((row & 524289) === 0) statusName = \"待确认\";\r\n\t\t\t\tif ((row & 1) > 0) statusName = \"预约成功\";\r\n\t\t\t\tif ((row & 524288) > 0) statusName = \"预约失败\";\r\n\t\t\t\tif ((row & 8) > 0) statusName = \"已取消\";\r\n\r\n\t\t\t\treturn statusName;\r\n\t\t\t},\r\n\t\t\t//邀请视音频通话\r\n\t\t\tinvitation(id) {\r\n\t\t\t\tvar msg = {\r\n\t\t\t\t\tto: id,\r\n\t\t\t\t\tinfo: {\r\n\t\t\t\t\t\tCreater: 1\r\n\t\t\t\t\t},\r\n\t\t\t\t\ttype: \"\"\r\n\t\t\t\t};\r\n\t\t\t\tthis.$even.$emit(\"user_live_launch\", msg);\r\n\t\t\t\t//this.noticeEx.send(msg.to,msg.info);\r\n\t\t\t},\r\n\t\t\tonShowHealthRecordDtlDialog(record) {\r\n\t\t\t\tthis.selectHealthRecord = record;\r\n\t\t\t\tthis.healthRecordDtlDialog = true;\r\n\t\t\t},\r\n\r\n\t\t\tgetAppointmentResult(gaeaObjectProperty) {\r\n\t\t\t\tif ((gaeaObjectProperty & 1) > 0) return true;\r\n\r\n\t\t\t\tif ((gaeaObjectProperty & 524288) > 0) return false;\r\n\r\n\t\t\t\treturn null;\r\n\t\t\t},\r\n\t\t\tonConfirmResultChange(v) {\r\n\t\t\t\t//this.rules.FeeAmount[0].required = v;\r\n\t\t\t\tthis.rules.FailResult[0].required = v === false;\r\n\r\n\t\t\t\t//this.confirmObj.FeeAmount = \"\";\r\n\t\t\t\tthis.confirmObj.FailResult = \"\";\r\n\t\t\t},\r\n\t\t\tonTabClick(tab) {\r\n\t\t\t\tconsole.log(tab.name);\r\n\t\t\t},\r\n\t\t\tonResetForm() {\r\n\t\t\t\tthis.confirmObj = {\r\n\t\t\t\t\tGaeaObjectProperty: 0,\r\n\t\t\t\t\tConfirmResult: null,\r\n\t\t\t\t\tFeeAmount: \"\",\r\n\t\t\t\t\tFailResult: \"\"\r\n\t\t\t\t};\r\n\t\t\t\tif (this.$refs.confirmForm) {\r\n\t\t\t\t\tthis.$refs.confirmForm.resetFields();\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tonConfirmClick(item) {\r\n\t\t\t\tthis.onResetForm();\r\n\t\t\t\tthis.currentObj = item;\r\n\r\n\t\t\t\tconst tempRecord = JSON.clone(item);\r\n\t\t\t\tObject.assign(this.confirmObj, tempRecord);\r\n\t\t\t\tthis.confirmObj.ConfirmTargetDate = this.confirmObj.AppointmentDate;\r\n\r\n\t\t\t\tthis.confirmObj.FeeAmount =\r\n\t\t\t\t\tthis.confirmObj.FeeAmount === 0 ? \"\" : this.confirmObj.FeeAmount;\r\n\r\n\t\t\t\tthis.confirmObj.ConfirmResult = this.getAppointmentResult(\r\n\t\t\t\t\tthis.confirmObj.GaeaObjectProperty\r\n\t\t\t\t);\r\n\t\t\t\t///this.confirmObj.ConfirmResult = this.appointmentResult\r\n\t\t\t\tthis.confirmDialog = true;\r\n\t\t\t},\r\n\t\t\tonCancelClick() {\r\n\t\t\t\tthis.confirmDialog = false;\r\n\t\t\t},\r\n\t\t\tonOkClick() {\r\n\t\t\t\tif (this.confirmObj.ConfirmResult) {\r\n\t\t\t\t\tthis.confirmObj.GaeaObjectProperty =\r\n\t\t\t\t\t\tthis.confirmObj.GaeaObjectProperty | 1;\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.confirmObj.GaeaObjectProperty =\r\n\t\t\t\t\t\tthis.confirmObj.GaeaObjectProperty | 524288;\r\n\r\n\t\t\t\t\t//delete this.confirmObj.FeeAmount;\r\n\t\t\t\t}\r\n\r\n\t\t\t\tthis.saveAppointmentConfirm();\r\n\t\t\t\t//this.confirmDialog = false\r\n\t\t\t},\r\n\t\t\t//更新支付状态\r\n\t\t\tasync updateAppointmentPayStatus(){\r\n\t\t\t\tlet params = {\r\n\t\t\t\t\tAppointmentGuid:this.selectCustomer.AppointmentGuid,\r\n\t\t\t\t\tPayStatus:this.selectCustomer.PayStatus == 0?1:0,\r\n\t\t\t\t}\r\n\t\t\t\tlet res = await this.$api.post(\"/Medical/UpdateAppointmentPaymentStatus\",params);\r\n\t\t\t\tif(!res.IsPositive){\r\n\t\t\t\t\tthis.$tips.warning(\"更新数据异常\");\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tthis.$tips.success(\"更新成功\");\r\n\t\t\t\tthis.view.payStatus = false;\r\n\t\t\t\tthis.selectCustomer.PayStatus = params.PayStatus;\r\n\t\t\t},\r\n\t\t\tasync queryData() {\r\n\t\t\t\tlet params = {\r\n\t\t\t\t\tPageIndex:this.pages.page,\r\n\t\t\t\t\tPageSize:this.pages.size,\r\n\t\t\t\t\tRowFilter:this.keyword,\r\n\t\t\t\t}\r\n\t\t\t\tthis.view.dataIsLoading = true;\r\n\t\t\t\tlet res = await this.$api.get(\"/Medical/QueryAppointment\",params);\r\n\t\t\t\tthis.view.dataIsLoading = false;\r\n\t\t\t\tif (!res.IsPositive) {\r\n\t\t\t\t\tthis.$tips.error(\"查询数据异常\");\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tthis.allData = res.Data.DataList;\r\n\t\t\t\tthis.seekAppointmentData.data = res.Data.DataList.filter(item => {\r\n\t\t\t\t\treturn item.AppointmentType === \"Examine\";\r\n\t\t\t\t});\r\n\t\t\t\tthis.goJpaAppointmentData.data = res.Data.DataList.filter(item => {\r\n\t\t\t\t\treturn item.AppointmentType === \"Journey\";\r\n\t\t\t\t});\r\n\t\t\t\tthis.pages.total = res.Data.TotalCount\r\n\t\t\t\t//this.seekAppointmentData.total = this.seekAppointmentData.data.length;\r\n\t\t\t\t//this.goJpaAppointmentData.total = this.goJpaAppointmentData.data.length;\r\n\t\t\t},\r\n\t\t\tsaveAppointmentConfirm() {\r\n\t\t\t\tconst _this = this;\r\n\t\t\t\tthis.$refs.confirmForm.validate(valid => {\r\n\t\t\t\t\tif (valid) {\r\n\t\t\t\t\t\tthis.$loading();\r\n\t\t\t\t\t\t_this.$api\r\n\t\t\t\t\t\t\t.post(\"/Medical/ConfirmAppointment\", this.confirmObj)\r\n\t\t\t\t\t\t\t.then(res => {\r\n\t\t\t\t\t\t\t\tthis.$loading().close();\r\n\t\t\t\t\t\t\t\tif (!res.IsPositive) {\r\n\t\t\t\t\t\t\t\t\t_this.$tips.error(\"保存失败\");\r\n\t\t\t\t\t\t\t\t\treturn;\r\n\t\t\t\t\t\t\t\t}\r\n\r\n\t\t\t\t\t\t\t\tlet record = _this.allData.filter(item => {\r\n\t\t\t\t\t\t\t\t\treturn (\r\n\t\t\t\t\t\t\t\t\t\titem.AppointmentGuid === _this.confirmObj.AppointmentGuid\r\n\t\t\t\t\t\t\t\t\t);\r\n\t\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t\t\tif (record.length === 0) return;\r\n\r\n\t\t\t\t\t\t\t\trecord[0].GaeaObjectProperty =\r\n\t\t\t\t\t\t\t\t\t_this.confirmObj.GaeaObjectProperty;\r\n\t\t\t\t\t\t\t\t//record[0].FeeAmount = _this.confirmObj.FeeAmount;\r\n\t\t\t\t\t\t\t\trecord[0].FailResult = _this.confirmObj.FailResult;\r\n\r\n\t\t\t\t\t\t\t\t// _this.seekAppointmentData = _this.allData.filter((item) => {\r\n\t\t\t\t\t\t\t\t//   return item.AppointmentType === 'Examine'\r\n\t\t\t\t\t\t\t\t// })\r\n\t\t\t\t\t\t\t\t// _this.goJpaAppointmentData = _this.allData.filter((item) => {\r\n\t\t\t\t\t\t\t\t//   return item.AppointmentType === 'Journey'\r\n\t\t\t\t\t\t\t\t// })\r\n\r\n\t\t\t\t\t\t\t\t_this.confirmDialog = false;\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t}\r\n\t};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"pages/main/appointment"}]}