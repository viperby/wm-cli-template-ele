OAAA;AAKA,WAAA,KAAA,CAAA,aAAA,CAAA,QAAA;AAAA,2EAAA,iBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAAA;AAAA,yBAIA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,4BAAA,EAAA,MAAA,CAJA;;AAAA;AAIA,kBAAA,GAJA;;AAAA,wBAMA,GAAA,CAAA,UAAA,IAAA,KANA;AAAA;AAAA;AAAA;;AAOA;AACA;AACA;AACA;AACA;AACA;AACA,kBAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,SAAA;;AAbA;;AAAA;AAgBA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA,EAhBA,CAiBA;;;AACA,kBAAA,MAAA,CAAA,MAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAoBA;AAtCA,GA3FA;AAmIA,EAAA,OAnIA,qBAmIA,CAEA;AArIA,CAAA","sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <el-row class=\"personal\" type=\"flex\" justify=\"center\">\r\n      <el-col :span=\"8\"><h1>个人信息</h1></el-col>\r\n    </el-row>\r\n    <el-row class=\"personal\" type=\"flex\" justify=\"center\">\r\n      <el-col :offset=\"1\" :span=\"8\">{{ `登录账号： ${uInfo.PhoneNO}` }}</el-col>\r\n    </el-row>\r\n    <el-row class=\"personal\" type=\"flex\" justify=\"center\">\r\n      <el-col :offset=\"1\" :span=\"8\">{{ `姓名： ${uInfo.UserName}` }}</el-col>\r\n    </el-row>\r\n    <!-- 表单区域 -->\r\n    <el-row class=\"update\" type=\"flex\" justify=\"center\">\r\n      <el-col :span=\"8\">\r\n        <h1>修改密码</h1>\r\n        <el-form ref=\"ChangeFormRef\"\r\n                 :model=\"ChangeFrom\"\r\n                 :rules=\"ChangeFromRules\"\r\n                 label-width=\"100px\">\r\n          <!-- 登录密码 -->\r\n          <el-form-item label=\"登录密码：\"\r\n                        prop=\"loginPassWord\">\r\n            <el-input v-model=\"ChangeFrom.loginPassWord\"\r\n                      placeholder=\"请输入密码\" \r\n                      show-password\r\n                      clearable>\r\n            </el-input>\r\n          </el-form-item>\r\n          <!-- 密码 -->\r\n          <el-form-item label=\"新密码：\"\r\n                        prop=\"passWord\">\r\n            <el-input v-model=\"ChangeFrom.passWord\"\r\n                      placeholder=\"请输入密码\"\r\n                      show-password\r\n                      clearable>\r\n            </el-input>\r\n            <!-- 密码强度条 -->\r\n            <el-progress style=\"margin-left: 2rem;\"\r\n                         v-if=\"percentage\"\r\n                         :percentage=\"percentage\" \r\n                         :color=\"customColors\"\r\n                         :format=\"passwordPercentFormat\"\r\n            ></el-progress>\r\n          </el-form-item>\r\n          <!-- 密码强度条 -->\r\n          \r\n          <!-- 确认密码 -->\r\n          <el-form-item label=\"确认密码：\"\r\n                        prop=\"checkPassWord\">\r\n            <el-input v-model=\"ChangeFrom.checkPassWord\"\r\n                      placeholder=\"请输入密码\"    \r\n                      show-password\r\n                      clearable>\r\n            </el-input>\r\n          </el-form-item>\r\n       </el-form>\r\n      </el-col>\r\n    </el-row>\r\n     <!-- 确认区域 -->\r\n    <el-row class=\"footer\" type=\"flex\" justify=\"center\">\r\n      <el-col :span=\"8\">\r\n        <el-button @click=\"cancel\">取消</el-button>\r\n        <el-button @click=\"updatePassWord\" type=\"primary\">确认</el-button>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n\tmapState,\r\n\tmapMutations\r\n} from 'vuex'\r\nexport default {\r\n  computed: {\r\n\t\t...mapState([\"uInfo\"]),\r\n  },\r\n  data() {\r\n    // 密码\r\n    let validatePass = (rule, value, callback) => {\r\n      var ls = 0;\r\n      // 正则 6-12英文数字组合密码 /^(?=.*[0-9])(?=.*[a-zA-Z])(.{6,12})$/\r\n      let reg = /^(?=.*[0-9])(.{6,20})$/\r\n      if (value === '') {\r\n        callback(new Error('请输入密码！'));\r\n      }\r\n      // 判断密码难度\r\n      if (this.ChangeFrom.passWord.match(/([a-z])+/)) { // 是否存在小写英文\r\n        ls += 1\r\n      }\r\n      if (this.ChangeFrom.passWord.match(/([0-9])+/)) { // 是否存在数字\r\n        ls += 1\r\n      }\r\n      if (this.ChangeFrom.passWord.match(/([A-Z])+/)) { // 是否存在大写英文\r\n        ls += 1\r\n      }\r\n      if (this.ChangeFrom.passWord.match(/([\\W])+/)) { // 是否存在除下划线的特殊符号 \r\n        ls += 1\r\n      }\r\n      if (this.ChangeFrom.checkPassWord !== '') {\r\n        this.$refs.ChangeFormRef.validateField('checkPassWord');\r\n      } else if (!reg.test(value)) {\r\n        callback(new Error('请输入6-20位密码'));\r\n      }\r\n      switch (ls) {\r\n        case 0:\r\n          this.percentage = 0;\r\n          break;\r\n        case 1:\r\n          this.percentage = 25;\r\n          break;\r\n        case 2:\r\n          this.percentage = 50;\r\n          break;\r\n        case 3:\r\n          this.percentage = 75;\r\n          break;\r\n        case 4:\r\n          this.percentage = 100;\r\n          break;      \r\n        default:\r\n          this.percentage = 0;\r\n          break;\r\n      }\r\n      callback(); \r\n    };\r\n    // 确认密码\r\n    let validatePass2 = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请再次输入密码'));\r\n      } else if (value !== this.ChangeFrom.passWord) {\r\n        callback(new Error('两次输入密码不一致!'));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      ChangeFrom: {\r\n        loginPassWord: '',// 登录密码\r\n        passWord: '',     // 新密码\r\n        checkPassWord: '' // 确认密码\r\n      },\r\n      // 表单验证对象\r\n      ChangeFromRules:{\r\n        loginPassWord:[\r\n          { required: true, message: '请输入登录密码', trigger: 'blur' },\r\n        ],\r\n        passWord: [\r\n          { required: true, validator: validatePass, trigger: 'blur' },\r\n          // { required: true, validator: validatedifficulty, trigger: 'input' }\r\n        ],\r\n        checkPassWord: [\r\n          { required: true, validator: validatePass2, trigger: 'blur' }\r\n        ],\r\n      },\r\n      percentage: 0,\r\n      customColors: [\r\n        {color: '#f56c6c', percentage: 26},\r\n        {color: '#e6a23c', percentage: 51},\r\n        {color: '#6f7ad3', percentage: 76},\r\n        {color: '#5cb87a', percentage: 100}\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    // 取消进度条默认样式\r\n    passwordPercentFormat() {\r\n      return \"\"\r\n    },\r\n    // 取消 并清除进度条\r\n    cancel() {\r\n      this.percentage = 0\r\n      this.$refs.ChangeFormRef.resetFields()\r\n    },\r\n    // 确定\r\n    updatePassWord() {\r\n      // MD5加密后再发送\r\n      let params = {\r\n        OriginPassword: this.$md5(this.ChangeFrom.loginPassWord),\r\n        NewPassword: this.$md5(this.ChangeFrom.passWord),\r\n        ConfirmNewPassword: this.$md5(this.ChangeFrom.checkPassWord)\r\n      }\r\n      this.$refs.ChangeFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        // console.log(\"验证通过\");\r\n        // 发起修改请求\r\n        let res = await this.$api.post('/Sobiz/ModifyLoginPassword', params)\r\n        // console.log(res);\r\n        if(res.IsPositive == false) {\r\n          // if(res.ErrorCode == \"ErrPassword\") {\r\n          //   this.$tips.warning(\"密码错误！\");\r\n          // }\r\n          // if(res.ErrorCode == \"NoChangePwd\") {\r\n          //   this.$tips.warning(\"当前密码与修改密码相同！\");\r\n          // }\r\n          this.$tips.warning(\"修改密码失败！\");\r\n          return\r\n        }\r\n        this.$message.success('密码修改成功！')\r\n        // 清空\r\n        this.cancel()\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n    \r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .content {\r\n    width: 100%;\r\n    .el-row {\r\n      margin-top: 2.5rem;\r\n    }\r\n    .footer {\r\n      text-align: center;\r\n      .el-button {\r\n        margin-right: 40px;\r\n        \r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"pages/main/info"}]}