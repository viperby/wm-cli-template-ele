fo\" :rules=\"rules\" ref=\"doctorForm\">\r\n\t\t\t\t\t<!-- 登录账号 -->\r\n\t\t\t\t\t<el-form-item v-if=\"!isEdit\" :label=\"$t('default.BASIC.ACCOUNT')\" prop=\"DoctorLoginCode\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-input v-model=\"doctorInfo.DoctorLoginCode\" :placeholder=\"$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:$t('default.BASIC.ACCOUNT')})\" maxlength=\"20\" show-word-limit autocomplete=\"off\" style=\"width: 300px;\"></el-input>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<el-form-item v-if=\"isEdit\" :label=\"$t('default.BASIC.ACCOUNT')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<span>{{ doctorInfo.UserCode }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 医生姓名 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.MODULE.DEPARTMENT.DOCTOR_NAME')\" prop=\"DoctorName\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-input v-model=\"doctorInfo.DoctorName\" :placeholder=\"$t('default.MODULE.DEPARTMENT.INPUT_DOCTOR_NAME')\" maxlength=\"10\" show-word-limit autocomplete=\"off\" style=\"width: 300px;\"></el-input>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 性别 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.INFOS.GENDE')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-radio-group v-model=\"doctorInfo.Gender\">\r\n\t\t\t\t\t\t\t<el-radio :label=\"$t('default.INFOS.MAN')\">{{ $t('default.INFOS.MAN') }}</el-radio>\r\n\t\t\t\t\t\t\t<el-radio :label=\"$t('default.INFOS.WOMAN')\">{{ $t('default.INFOS.WOMAN') }}</el-radio>\r\n\t\t\t\t\t\t</el-radio-group>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 所属科室 -->\r\n\t\t\t\t\t<el-form-item v-if=\"!isEdit\" :label=\"$t('default.MODULE.DEPARTMENT.AFFILIATED_DEPARTMENT')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-select v-model=\"doctorInfo.OfficeTypeGuid\" multiple collapse-tags :placeholder=\"$t('default.TIPS.CLIENT.SELECT_SOMETHING',{name:$t('default.MODULE.DEPARTMENT.AFFILIATED_DEPARTMENT')})\" style=\"width: 300px;\">\r\n\t\t\t\t\t\t\t<!-- <el-option :label=\"$t('default.EVENTS.UNSELECT')\" :value=\"$null\"></el-option> -->\r\n\t\t\t\t\t\t\t<!-- <el-option v-for=\"(item,index) in departmentNode\" :key=\"index\" :label=\"item.OfficeTypeName\" :value=\"item.OfficeTypeGuid\"></el-option> -->\r\n\t\t\t\t\t\t\t<el-option-group\r\n\t\t\t\t\t\t\t\tv-for=\"(item,index) in department\"\r\n\t\t\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t\t\t:label=\"item.OfficeTypeName\">\r\n\t\t\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\t\t\tv-for=\"(sub,subIndex) in item.SubTypeList\"\r\n\t\t\t\t\t\t\t\t\t:key=\"subIndex\"\r\n\t\t\t\t\t\t\t\t\t:label=\"sub.OfficeTypeName\"\r\n\t\t\t\t\t\t\t\t\t:value=\"sub.OfficeTypeGuid\">\r\n\t\t\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t\t\t</el-option-group>\r\n\t\t\t\t\t\t</el-select>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 所属医院 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.MODULE.DEPARTMENT.AFFILIATED_HOSPITAL')\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-select v-model=\"doctorInfo.HospitalGuid\" :placeholder=\"$t('default.TIPS.CLIENT.SELECT_SOMETHING',{name:$t('default.MODULE.DEPARTMENT.AFFILIATED_HOSPITAL')})\" >\r\n\t\t\t\t\t\t\t<el-option :label=\"$t('default.EVENTS.UNSELECT')\" :value=\"$null\"></el-option>\r\n\t\t\t\t\t\t\t<el-option v-for=\"(item,index) in hospitalData\" :key=\"index\" :label=\"item.HospitalName\" :value=\"item.HospitalGuid\"></el-option>\t\r\n\t\t\t\t\t\t</el-select>\r\n\t\t\t\t\t\t<el-button slot=\"append\" icon=\"el-icon-search\"></el-button>\r\n\t\t\t\t\t\t<el-button @click=\"addHospital\" size=\"small\" type=\"success\" icon=\"el-icon-plus\" circle style=\"position: absolute;margin-left: 10px;margin-top: 5px;\"></el-button>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 医生照片 -->\r\n\t\t\t\t\t<el-form-item label=\"医生照片\" label-width=\"100px\">\r\n\t\t\t\t\t\t<el-upload\r\n\t\t\t\t\t\t\tclass=\"avatar-uploader\"\r\n\t\t\t\t\t\t\t:action=\"uploadUrl\"\r\n\t\t\t\t\t\t\t:data=\"uploadData\"\r\n\t\t\t\t\t\t\taccept=\"image/jpeg, image/jpg\"\r\n\t\t\t\t\t\t\t:show-file-list=\"false\"\r\n\t\t\t\t\t\t\t:on-success=\"onImgUpload\">\r\n\t\t\t\t\t\t\t<img v-if=\"doctorInfo.ImagePath\" :src=\"serverAddr+doctorInfo.ImagePath\" class=\"avatar\">\r\n\t\t\t\t\t\t\t<i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n\t\t\t\t\t\t</el-upload>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t\t<!-- 说明 -->\r\n\t\t\t\t\t<el-form-item :label=\"$t('default.BASIC.DESCRIPTION')\" prop=\"DoctorIntroduction\" label-width=\"100px\">\r\n\t\t\t\t\t\t<div ref=\"description\" style=\"height:250px\"></div>\r\n\t\t\t\t\t\t<!-- <el-input v-model=\"doctorInfo.DoctorDescription\" resize=\"none\" type=\"textarea\" :placeholder=\"$t('default.TIPS.CLIENT.INPUT_SOMETHING',{name:$t('default.BASIC.DESCRIPTION')})\" maxlength=\"150\" show-word-limit style=\"width: 300px;\"></el-input> -->\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-form>\r\n\t\t\t</el-col>\r\n\t\t</el-row>\r\n\t\t<div slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t<el-button @click=\"layzValue = false\">{{ $t('default.BASIC.CANCEL') }}</el-button>\r\n\t\t\t<el-button type=\"primary\" @click=\"doctorEnter()\">{{ $t('default.BASIC.ENTER') }}</el-button>\r\n\t\t</div>\r\n\t</el-dialog>\n</template>\n\n<script>\r\n\timport conf from '@/conf/conf.js'\r\n\timport E from 'wangeditor'\r\n\texport default {\r\n\t\tname:\"docotr\",\r\n\t\tprops:{\r\n\t\t\tisEdit:{\r\n\t\t\t\ttype:Boolean,\r\n\t\t\t\tdefault:false,\r\n\t\t\t},\r\n\t\t\tvalue:{\r\n\t\t\t\ttype:Boolean,\r\n\t\t\t\tdefault:false,\r\n\t\t\t},\r\n\t\t\tdocotrGuid:{\r\n\t\t\t\ttype:String,\r\n\t\t\t\tdefault:\"\"\r\n\t\t\t}\r\n\t\t},\r\n\t\twatch:{\r\n\t\t\tvalue(){\r\n\t\t\t\tthis.layzValue = this.value;\r\n\t\t\t\t\r\n\t\t\t\tif(this.value && this.isFirst){\r\n\t\t\t\t\tthis.isFirst = false;\r\n\t\t\t\t\tsetTimeout(()=>{\r\n\t\t\t\t\t\tthis.initEdit();\r\n\t\t\t\t\t},200)\t\t\t\t\r\n\t\t\t\t}\r\n\t\t\t\tif(this.value && !this.isEdit){\r\n\t\t\t\t\tthis.editor.txt.clear();\r\n\t\t\t\t\tthis.doctorInfo={\r\n\t\t\t\t\t\tDoctorName:\"\",\r\n\t\t\t\t\t\tDoctorLoginCode:\"\",\r\n\t\t\t\t\t\tGender:this.$t('default.INFOS